package Mx_star.PizzaIR;

import java.util.*;

class Variable {
    String name, type;

    Variable(String name, String type) {
        this.name = name;
        this.type = type;
    }

    String getName() {
        return name;
    }

    String getType() {
        return type;
    }
}

class VariableList {
    Vector<Variable> list = new Vector<Variable>();

    boolean hasVariable(String name) {
        for (var variable : list) {
            if (variable.name.equals(name)) {
                return true;
            }
        }
        return false;
    }

    String getType(String name) {
        for (var variable : list) {
            if (variable.name.equals(name)) {
                return variable.type;
            }
        }
        return null;
    }
}

